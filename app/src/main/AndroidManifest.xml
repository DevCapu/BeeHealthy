<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.devcapu.beehealthy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BeeHealthy">
        <activity
            android:name="br.com.devcapu.beehealthy.main.MainActivity"
            android:theme="@style/Theme.BeeHealthy" />

        <activity
            android:name="br.com.devcapu.beehealthy.authentication.login.ui.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="br.com.devcapu.beehealthy.authentication.register.ui.RegisterActivity"
            android:theme="@style/Theme.BeeHealthy" />
    </application>
</manifest>